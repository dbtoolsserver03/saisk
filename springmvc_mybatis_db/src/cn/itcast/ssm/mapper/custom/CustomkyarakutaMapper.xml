<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.itcast.ssm.mapper.custom.CustomkyarakutaMapper">
  	<select id="findkyarakutaList" parameterType="cn.itcast.ssm.po.original.KyarakutaTable"
  		 resultMap="cn.itcast.ssm.mapper.original.KyarakutaTableMapper.BaseResultMap">
  		SELECT * FROM kyarakuta_table
  		<where>
   			<if test="kyarakutaName!=null and kyarakutaName != ''">
   				kyarakuta_name LIKE '%${kyarakutaName}%'
   			</if>

  		</where>
  	</select>

</mapper>